<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="0">
  
        <!-- sequence of claim -->
    	<record id="sequence_claim_seq" model="ir.sequence">
            <field name="name">Numéro Réclamation</field>
            <field name="code">oscar.sav.client.claim</field>
            <field name="prefix">RC/%(range_year)s/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">5</field>
        </record>
        
        <!-- sequence of prefix RI-->
    	<record id="sequence_prefix_ri_seq" model="ir.sequence">
            <field name="name">Numéro Réclamation</field>
            <field name="code">oscar.sav.internal.claim</field>
            <field name="prefix">RI/%(range_year)s/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">5</field>
        </record>
        
        <!-- sequence de bon de sortie -->
    	<record id="sequence_bon_sortie_seq" model="ir.sequence">
            <field name="name">Numéro Bon de Sortie</field>
            <field name="code">oscar.sav.bon.sortie.wizard</field>
            <field name="prefix">BS/%(range_year)s/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">5</field>
        </record>
        
       
		
