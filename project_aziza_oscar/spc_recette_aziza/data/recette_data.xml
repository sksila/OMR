        <!-- sequence of versement -->
    	<record id="sequence_versement_seq" model="ir.sequence">
            <field name="name">Numéro versement</field>
            <field name="code">oscar.versement</field>
            <field name="prefix">VE/%(range_year)s/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">5</field>
        </record>
        
        <!-- sequence of versement cheques -->
    	<record id="sequence_versement_cheque_seq" model="ir.sequence">
