<?xml version="1.0"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="ha_dispatching_view_users_form_inhereted">
            <field name="name">res.users.form</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page [2]" position="after">
                    <page string="Approbateurs">
                        <group string="Approbateurs dispatch" name="dispatch" groups="spc_oscar_aziza.group_admin_oscar">
                            <field name="dispatch_approbateurs"/>
                        </group>
                    </page>
                </xpath>

            </field>
        </record>

    </data>
</odoo>
